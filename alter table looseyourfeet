
use elanop4y_fest_2017;

alter table elanejung 
    ADD COLUMN name text,
    ADD COLUMN mobile varchar(13),
    ADD COLUMN  attendance varchar(3) DEFAULT 'NO',
    ADD COLUMN winner varchar(3)  DEFAULT 'NO',
    ADD COLUMN comment text; 


UPDATE elanejung table1 SET 
table1.name = (SELECT table2.name FROM users table2 WHERE table1.ID = table2.ID),
table1.mobile = (SELECT table2.mobile FROM users table2 WHERE table1.ID = table2.ID);




